{
  "name": "brewcompetition-cli",
  "version": "1.0.0",
  "description": "Brew Competition - CLI to fetch medal earners and various homebrew competition metadata.",
  "keywords": [
    "homebrew",
    "beer",
    "competition",
    "medals",
    "brewing",
    "bjcp",
    "bcoem",
    "reggie",
    "bap",
    "cli",
    "results",
    "awards",
    "oclif"
  ],
  "homepage": "https://github.com/tmack8001/brewcompetition-cli",
  "bugs": "https://github.com/tmack8001/brewcompetition-cli/issues",
  "repository": "tmack8001/brewcompetition-cli",
  "license": "MIT",
  "author": "Trevor Mack @tmack8001",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "brewcompetition": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "install-hooks": "sh scripts/install-hooks.sh",
    "lint": "eslint . --ext .ts",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "release": "sh scripts/release.sh",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md CHANGELOG.md",
    "postversion": "echo 'Version updated. Push with: git push origin main --tags'"
  },
  "oclif": {
    "bin": "brewcompetition",
    "commands": "./dist/commands",
    "dirname": "brewcompetition",
    "topicSeparator": " ",
    "topics": {
      "medals": {
        "description": "fetch medals from a specific homebrew competition"
      },
      "competitions": {
        "description": "fetch metadata (entry status, entry deadlines, drop off window, awards, etc) from a specific homebrew competition"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "moment-timezone": "^0.5.45",
    "node-persist": "^4.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/json2csv": "^5.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/node-fetch": "^2.6.11",
    "@types/node-localstorage": "^1.3.3",
    "@types/node-persist": "^3.1.8",
    "@types/uuid": "^9.0.8",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "mocha": "^10",
    "node-localstorage": "^3.0.5",
    "oclif": "^4.4.19",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}